%define MACH_SYSCALL(nb)	0x2000000 | nb
%define STDOUT			1
%define WRITE			4

section .data
yo:
	.string db "(null)", 10

section .text
	global _ft_puts
	extern _ft_strlen

_ft_puts:
	mov rsi, rdi
	call _ft_strlen ;store len in rax

	mov rdi, STDOUT ; output (fd)
	mov rdx, rax ; size
	mov rax, MACH_SYSCALL(WRITE) ; syscall
	syscall
	jc .error


	

	mov rax, 10
	jmp .end
.error:
	mov rax, -1
.end:
	ret
